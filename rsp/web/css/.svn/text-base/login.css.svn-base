body{
    margin:0;
	padding:0;
	font-size:12px;
	font-family:"宋体"
}
.login{
    width:664px;
	height:444px;
	margin-left:auto;
	margin-right:auto;
	margin-top:100px;
}
.top{
    background:url(../images/header1.jpg) no-repeat left top;
	width:664px;
	height:230px;
	float:left;
}
.bottom{
    width:664px;
	height:214px;
	background:url(../images/footer.jpg) no-repeat left top;
	float:left;
}
.warning{
    width:600px;
	height:25px;
	text-align:center;
	line-height:2em;
	color:#f00;
	font-size:14px;
	font-weight:bold;
	margin-left:auto;
	margin-right:auto;
	margin-top:5px;
}
.main{
    width:600px;
	height:120px;
	text-align:center;
	margin-left:auto;
	margin-right:auto;
}
.info{
    width:230px;
	height:85px;
	float:left;
	margin-left:110px;
	padding-top:15px;
}
.infoline{
    height:35px;
	width:230px;
	float:left;
	line-height:1.8em;
}
.user,.password,.code{
    width:55px;
	text-align:left;
	float:left;
}
.word{
    letter-spacing:1em;
}
.userinput,.passwordinput{
    width:155px;
	height:22px;
	float:left;
    text-align: left;
}

.codeinput{
    width:60px;
    height:22px;
    float:left;
    text-align: left;
}

.userinput input,.passwordinput input{
    float:left;
}
.button{
    width:100px;
	height:85px;
	float:left;
	padding-top:11px;
}
.buttonimage{
    margin-left:8px;
}


